  "requests": [
    {
      "name": "1. Register User - Valid",
      "method": "POST",
      "url": "{{baseUrl}}/api/auth/register",
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json"
        }
      ],
      "body": {
        "type": "json",
        "raw": "{\n  \"email\": \"{{testEmail}}\",\n  \"password\": \"Test1234!\",\n  \"full_name\": \"John Doe\",\n  \"gender\": \"m\",\n  \"mobile_no\": \"+919876543210\",\n  \"signup_type\": \"e\"\n}",
        "form": []
      },
      "tests": [
        {
          "type": "res-code",
          "value": "201"
        },
        {
          "type": "json-query",
          "value": "$.success",
          "expected": "true"
        },
        {
          "type": "json-query",
          "value": "$.data.user_id",
          "expected": "exists"
        }
      ]
    },
    {
